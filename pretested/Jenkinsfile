node {
  
  stage("Checkout"){
    checkout scm
    // git credentialsId: 'frederikmadsen', url: 'git@github.com:dajoh16/ca-project.git'
  }

  stage('Build'){
    sh 'docker build -t magida/codechan .'
  }

  stage('Test'){
    sh 'cat tests.py'
    sh 'docker container run -i --rm magida/codechan tests.py'
  }

}
